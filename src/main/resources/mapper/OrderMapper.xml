<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huang.datasource.mapper.order.OrderMapper">


    <sql id="AreaDicColumns">
		`name`
    </sql>



	 
	<insert id="save" parameterType="com.huang.datasource.domain.Order">
		insert into `order`
		(
			`name`
		)
		values
		(
			#{name}
		)
	</insert>

</mapper>